#include "led.h"
#include "pwm.h"






bool ledInit(void)
{
  return true;
}
void ledOn(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
     powmWrite(_DEF_CH1,0);
      break;
  }

}
void ledOff(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
      pwmWrite(_DEF_CH1,0);
      break;
  }

}
void ledToggle(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
      //HAL_GPIO_TogglePin(LED1_GPIO_Port, LED1_Pin);
      break;
  }

}

