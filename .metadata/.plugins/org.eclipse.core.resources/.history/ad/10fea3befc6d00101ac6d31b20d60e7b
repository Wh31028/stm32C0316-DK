#include "ap.h"


void apInit(void)
{
  ledInit();


}

void apMain(void)
{
  uint32_t pre_time;
  while(1)
  {
    if(HAL_GetTick()-pre_time >=500)
    {
      pre_time = HAL_GetTick();
      HAL_GPIO_TogglePin(LED1_GPIO_Port, LED1_Pin);
    }

  }
}
