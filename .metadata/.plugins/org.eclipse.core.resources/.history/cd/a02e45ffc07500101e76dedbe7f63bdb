#include "adc.h"


extern ADC_HandleTypeDef hadc1;


bool adcInit(void)
{
  return true;
}


uint16_t adcRead(uint8_t ch)
{
  uint16_t ret = 0;

  switch(ch)
  {
    case _DEF_CH1:
      HAL_ADC_Start(&hadc1);
      HAL_ADC_PollForConversion(&hadc1,10);
      adc_data = HAL_ADC_GetValue(&hadc1);
      break;
  }




}
