#include "led.h"
#include "pwm.h"

static uint16_t led_pwm[1] = {100};




bool ledInit(void)
{
  return true;
}
void ledOn(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
     pwmWrite(_DEF_CH1,0);
      break;
  }

}
void ledOff(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
      pwmWrite(_DEF_CH1,100);
      break;
  }

}
void ledToggle(uint8_t ch)
{
  switch(ch)
  {
    case _DEF_CH1:
      if(pwmRead(_DEF_CH1) > 0 )
        ledOn(_DEF_CH1);
      else
        ledOff(_DEF_CH1);
      break;
  }

}

void ledSetPwm(uint8_t ch,uint16_t pwm_data)
{
  switch(ch)
    {
      case _DEF_CH1:

        break;
    }
}

